<div class="container">

  <div class="row page-title">
    <h1>Itineraries#index</h1>
    <p>Find me in app/views/itineraries/index.html.erb</p>
  </div>




  <% @itineraries.each do |itinerary|%>

    <div class="card mb-3" style="max-width: 590px;">
      <div class="row g-0">
        <div class="col-md-4">
          <img src="https://upload.wikimedia.org/wikipedia/commons/1/11/GLPK_solution_of_a_travelling_salesman_problem.svg" class="img-fluid rounded-start" alt="...">
        </div>
        <div class="col-md-8">
          <div class="card-body">
            <h5 class="card-title"><%= itinerary.name %><%= itinerary.id %>
              </h5>
            <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
            <p class="card-text"><small class="text-muted">Created at <br> <%=itinerary.created_at%></small></p>
          </div>
        </div>
      </div>
    </div>
  <% end %>


</div>
